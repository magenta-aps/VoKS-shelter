V=production
#V=local
APP_ENV=production
#APP_ENV=local
APP_DEBUG=false
APP_KEY=<generate with "php artisan key:generate">

DB_HOST=127.0.0.1
DB_DATABASE=<database name>
DB_USERNAME=<mysql user username>
DB_PASSWORD=<mysql user password>

CACHE_DRIVER=seconds
SESSION_DRIVER=file
QUEUE_DRIVER=database

MAIL_DRIVER=mail
MAIL_HOST=<mail server smtp>
MAIL_PORT=<mail server smtp port>
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_FROM=
MAIL_FROM_NAME=

SSL_ON=true
PHP_WS_URL=<shelter domain name>
WOL_PORT=7
SHELTER_URL=<server ip address>
#SCHOOL_ID=1

# Active directory
AD_ENABLED=true
AD_USERNAME=<ad server username>
AD_GROUP=<ad parent crisis team group>
AD_PASSWORD=<ad server pass>
AD_ACCOUNT_SUFFIX=@example.com
AD_HOST=ldap.example.com
AD_DN='dc=<name>,dc=com'
AD_BIOS=<name>
AD_COMPANY_ID=<ID>

# Aruba
ARUBA_ALE_ENABLED=false
ARUBA_ALE_URL=<aruba ale server IP>
ARUBA_ALE_USERNAME=<aruba ale user name>
ARUBA_ALE_PASSWORD=<aruba ale password>

ARUBA_CLEARPASS_ENABLED=false
ARUBA_CLEARPASS_URL=<aruba clearpass URL>
ARUBA_CLEARPASS_USERNAME=<aruba clearpass username>
ARUBA_CLEARPASS_PASSWORD=<aruba clearpass password>

ARUBA_AIRWAVE_ENABLED=false
ARUBA_AIRWAVE_URL=<aruba airwave URL>
ARUBA_AIRWAVE_USERNAME=<aruba airwave username>
ARUBA_AIRWAVE_PASSWORD=<aruba password username>

ARUBA_COORDINATES_ENABLED=true
COORDINATES_TIME_INTERVAL=120
COORDINATES_EXPIRATION_TIME=15

# Google Maps
GOOGLE_MAPS_ENABLED=true
GOOGLE_MAPS_KEY=<google maps key>
# Possible zoom values: 1 - World, 5 - Landmass/continent, 10 - City, 15 - Streets, 20 - Buildings
GOOGLE_MAPS_DEFAULT_ZOOM=15

# Cisco
CISCO_ENABLED=false

UCP_USERNAME=<ucp username>
UCP_PASSWORD=<ucp password>
UCP_BASE_URI=<full ucp url to admin interface>
UCP_API_USER=<ucp api username>
UCP_API_PASS=<ucp api password>

WS_LOG=/var/log/ws-server

